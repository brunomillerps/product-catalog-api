{
  "name": "product-catalog-api",
  "version": "1.0.0",
  "description": "Offer a product catalog APIs",
  "main": "src/app.ts",
  "repository": "git@github.com:brunomillerps/product-catalog-api.git",
  "author": "Bruno Miller",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir bin --copy-files --no-copy-ignored",
    "start": "NODE_ENV=production node -r dotenv/config bin/server.js dotenv_config_path=src/config/production.env",
    "dev": "NODE_ENV=development tsnd -r dotenv/config -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify ./src/server.ts dotenv_config_path=src/config/development.env",
    "test": "DOTENV_CONFIG_PATH=./src/config/test.env jest"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/node": "^17.0.23",
    "axios": "^0.26.1",
    "cockatiel": "^3.0.0-beta.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-joi-validation": "^5.0.1",
    "express-rate-limit": "^6.3.0",
    "express-winston": "^4.2.0",
    "flatted": "^3.2.5",
    "helmet": "^5.0.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "mongoose": "^6.2.10",
    "uuid": "^8.3.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.1",
    "@types/mongoose": "^5.11.97",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^27.5.1",
    "nock": "^13.2.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
